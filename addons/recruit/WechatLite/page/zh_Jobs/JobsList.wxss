/*   ZZZZzzzz....  Start */
#swiper-tab {
  width: 100%;
  position: relative;
}
 
.swiper-tab {
  width: 100%;
  text-align: center;
  line-height: 80rpx;
  background-color: white;
}
 
.swiper-tab-list {
  font-size: 24rpx;
  display: inline-block;
  width: 32%;
  color: #666;
  border-bottom: 0rpx;
  position: relative;
}
 
.swiper-tab-list i {
  position: absolute;
  bottom: 30%;
  right: 14%;
  width: 0px;
  height: 0px;
  border: 5px solid #4e4e4e;
  border-top-color: #4e4e4e;
  border-bottom-color: transparent;
  border-left-color: transparent;
  border-right-color: transparent;
}
 
.on {
  color: #f63;
  border-bottom: 2rpx solid #f63;
}
 
.on i {
  border: 5px solid #f63;
  border-top-color: #f63;
  border-top-color: transparent;
  border-left-color: transparent;
  border-right-color: transparent;
  top:24rpx;
}
 
.swiper-box {
  display: block;
  height: 100%;
  width: 100%;
  overflow: hidden;
}
 
.swiper-box view {
  text-align: center;
}
 
.swiper-item {
  background: #fff;
  width: 100%;
  height: 600rpx;
  display: none;
  position: absolute;
  top: 85rpx;
  z-index: 333333;
  left: 0;
  animation: displays 1s;
}
 
@keyframes displays {
  from {
    height: 0px;
  }
 
  to {
    height: 600rpx;
  }
}
 
.swiper-beijing {
  width: 100%;
  height: 200%;
  background: #000;
  opacity: 0.5;
  position: absolute;
  top:401rpx;
  left: 0;
  display: none;
  z-index: 888;
  animation: 1s;
}

.topbjin{
  top:0px;
  height: 301rpx;
  display: none;
}
 
 
